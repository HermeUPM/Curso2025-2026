@prefix : <http://example.org/group11/ontology/cultural-events#> .
@prefix ce-res: <http://example.org/group11/resource/cultural-events/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix wgs84: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

<http://example.org/group11/ontology/cultural-events>
    a owl:Ontology ;
    rdfs:label "Ontology for Madrid cultural agenda events"@en ;
    rdfs:comment "Lightweight ontology for representing cultural events from the Madrid open data agenda, their venues and related information."@en ;
    .

############################
# CLASSES
############################

:Event a owl:Class ;
    rdfs:label "Cultural event"@en ;
    rdfs:comment "An event from the Madrid cultural agenda (a single show, concert, exhibition day, etc.)."@en .

:Activity a owl:Class ;
    rdfs:label "Activity"@en ;
    rdfs:comment "A recurring or grouped activity under which one or more events are organized."@en .

:Venue a owl:Class ;
    rdfs:label "Venue"@en ;
    rdfs:comment "A physical facility or place where cultural events take place."@en .

:District a owl:Class ;
    rdfs:label "District"@en ;
    rdfs:comment "Administrative district of Madrid where a venue is located."@en .

:Neighborhood a owl:Class ;
    rdfs:label "Neighborhood"@en ;
    rdfs:comment "Neighborhood within a district of Madrid where a venue is located."@en .

:AudienceCategory a owl:Class ;
    rdfs:label "Audience category"@en ;
    rdfs:comment "Category of target audience of an event (e.g. children, families, youth)."@en .

:EventType a owl:Class ;
    rdfs:label "Event type"@en ;
    rdfs:comment "Type or category of event (the values from the TIPO column)."@en .

############################
# OBJECT PROPERTIES
############################

:hasVenue a owl:ObjectProperty ;
    rdfs:label "has venue"@en ;
    rdfs:domain :Event ;
    rdfs:range :Venue ;
    rdfs:comment "Relates an event to the venue where it takes place."@en .

:hasActivity a owl:ObjectProperty ;
    rdfs:label "has activity"@en ;
    rdfs:domain :Event ;
    rdfs:range :Activity ;
    rdfs:comment "Relates an event to the activity it belongs to (TITULO-ACTIVIDAD / URL-ACTIVIDAD)."@en .

:hasAudience a owl:ObjectProperty ;
    rdfs:label "has audience"@en ;
    rdfs:domain :Event ;
    rdfs:range :AudienceCategory ;
    rdfs:comment "Relates an event to one or more audience categories derived from the AUDIENCIA field."@en .

:hasEventType a owl:ObjectProperty ;
    rdfs:label "has event type"@en ;
    rdfs:domain :Event ;
    rdfs:range :EventType ;
    rdfs:comment "Relates an event to its type/category (TIPO column)."@en .

:isInDistrict a owl:ObjectProperty ;
    rdfs:label "is in district"@en ;
    rdfs:domain :Venue ;
    rdfs:range :District ;
    rdfs:comment "Relates a venue to the district where it is located."@en .

:isInNeighborhood a owl:ObjectProperty ;
    rdfs:label "is in neighborhood"@en ;
    rdfs:domain :Venue ;
    rdfs:range :Neighborhood ;
    rdfs:comment "Relates a venue to the neighborhood where it is located."@en .

############################
# DATATYPE PROPERTIES (EVENT)
############################

:eventId a owl:DatatypeProperty ;
    rdfs:label "event identifier"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:integer ;
    rdfs:comment "Identifier of the event (ID-EVENTO)."@en .

:title a owl:DatatypeProperty ;
    rdfs:label "title"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:string ;
    rdfs:comment "Title of the event (TITULO)."@en .

:price a owl:DatatypeProperty ;
    rdfs:label "price"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:string ;
    rdfs:comment "Price description of the event (PRECIO)."@en .

:isFree a owl:DatatypeProperty ;
    rdfs:label "is free"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates if the event is free (GRATUITO)."@en .

:isLongDuration a owl:DatatypeProperty ;
    rdfs:label "is long duration"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates if the event is considered long duration (LARGA-DURACION)."@en .

:daysOfWeek a owl:DatatypeProperty ;
    rdfs:label "days of week"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:string ;
    rdfs:comment "Days of the week when the event takes place (DIAS-SEMANA)."@en .

:excludedDays a owl:DatatypeProperty ;
    rdfs:label "excluded days"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:string ;
    rdfs:comment "Excluded days for long-duration events (DIAS-EXCLUIDOS)."@en .

:startDate a owl:DatatypeProperty ;
    rdfs:label "start date"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Start date of the event (FECHA)."@en .

:endDate a owl:DatatypeProperty ;
    rdfs:label "end date"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "End date of the event (FECHA-FIN)."@en .

:startTime a owl:DatatypeProperty ;
    rdfs:label "start time"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:time ;
    rdfs:comment "Start time of the event (HORA)."@en .

:description a owl:DatatypeProperty ;
    rdfs:label "description"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:string ;
    rdfs:comment "Textual description of the event (DESCRIPCION)."@en .

:eventPage a owl:DatatypeProperty ;
    rdfs:label "event page URL"@en ;
    rdfs:domain :Event ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "URL of the event information page (CONTENT-URL)."@en .

:activityTitle a owl:DatatypeProperty ;
    rdfs:label "activity title"@en ;
    rdfs:domain :Activity ;
    rdfs:range xsd:string ;
    rdfs:comment "Title of the activity (TITULO-ACTIVIDAD)."@en .

:activityPage a owl:DatatypeProperty ;
    rdfs:label "activity page URL"@en ;
    rdfs:domain :Activity ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "URL of the activity information page (URL-ACTIVIDAD)."@en .

############################
# DATATYPE PROPERTIES (VENUE)
############################

:venueName a owl:DatatypeProperty ;
    rdfs:label "venue name"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the venue (NOMBRE-INSTALACION)."@en .

:venuePage a owl:DatatypeProperty ;
    rdfs:label "venue page URL"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "URL of the venue information page (URL-INSTALACION)."@en .

:accessibilityInfo a owl:DatatypeProperty ;
    rdfs:label "accessibility information"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string ;
    rdfs:comment "Accessibility information of the venue (ACCESIBILIDAD-INSTALACION)."@en .

:streetType a owl:DatatypeProperty ;
    rdfs:label "street type"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string ;
    rdfs:comment "Type of street (CLASE-VIAL-INSTALACION)."@en .

:streetName a owl:DatatypeProperty ;
    rdfs:label "street name"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string ;
    rdfs:comment "Street name (NOMBRE-VIA-INSTALACION)."@en .

:streetNumber a owl:DatatypeProperty ;
    rdfs:label "street number"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string ;
    rdfs:comment "Street number of the venue (NUM-INSTALACION)."@en .

:postalCode a owl:DatatypeProperty ;
    rdfs:label "postal code"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string ;
    rdfs:comment "Postal code of the venue (CODIGO-POSTAL-INSTALACION)."@en .

:coordX a owl:DatatypeProperty ;
    rdfs:label "X coordinate"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:decimal ;
    rdfs:comment "X coordinate of the venue (COORDENADA-X)."@en .

:coordY a owl:DatatypeProperty ;
    rdfs:label "Y coordinate"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Y coordinate of the venue (COORDENADA-Y)."@en .

# Using existing WGS84 properties for lat/long
wgs84:lat a owl:DatatypeProperty ;
    rdfs:label "latitude"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Latitude of the venue (LATITUD)."@en .

wgs84:long a owl:DatatypeProperty ;
    rdfs:label "longitude"@en ;
    rdfs:domain :Venue ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Longitude of the venue (LONGITUD)."@en .

############################
# DATATYPE PROPERTIES (DISTRICT / NEIGHBORHOOD / AUDIENCE / TYPE)
############################

:districtName a owl:DatatypeProperty ;
    rdfs:label "district name"@en ;
    rdfs:domain :District ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the district (DISTRITO-INSTALACION)."@en .

:neighborhoodName a owl:DatatypeProperty ;
    rdfs:label "neighborhood name"@en ;
    rdfs:domain :Neighborhood ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the neighborhood (BARRIO-INSTALACION)."@en .

:audienceLabel a owl:DatatypeProperty ;
    rdfs:label "audience label"@en ;
    rdfs:domain :AudienceCategory ;
    rdfs:range xsd:string ;
    rdfs:comment "Label or code of an audience category (values from AUDIENCIA column)."@en .

:eventTypeLabel a owl:DatatypeProperty ;
    rdfs:label "event type label"@en ;
    rdfs:domain :EventType ;
    rdfs:range xsd:string ;
    rdfs:comment "Label of the event type (values from TIPO column)."@en .

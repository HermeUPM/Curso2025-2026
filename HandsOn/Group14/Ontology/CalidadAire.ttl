@prefix cal:  <http://datos.madrid.es/Calidad_Aire#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .


# Metadata
cal: a owl:Ontology ;
    rdfs:label "Calidad del Aire – Ontología generada para RML" ;
    rdfs:comment "Ontología alineada al RDF producido mediante el RML del dataset de calidad del aire." .


# Clases
cal:Estacion a owl:Class ;
    rdfs:label "Estación" ;
    rdfs:comment "Estación de monitorización de calidad del aire." .

cal:Medicion a owl:Class ;
    rdfs:label "Medición" ;
    rdfs:comment "Una medición realizada por una estación en un día concreto." .


# Propiedades de Estación
cal:ESTACION a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string ;
    rdfs:label "ID de estación" .

cal:CODIGO a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string ;
    rdfs:label "Código INE" .

cal:PROVINCIA a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string .

cal:MUNICIPIO a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string .

cal:NOMBRE_ESTACION a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string .

cal:DIRECCION_ESTACION a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string .

cal:DIR_ESTACION_LINK a owl:ObjectProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range rdfs:Resource ;
    rdfs:label "Enlace asociado a la estación" .

cal:ESTACION_LATITUD a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string .

cal:ESTACION_LONGITUD a owl:DatatypeProperty ;
    rdfs:domain cal:Estacion ;
    rdfs:range xsd:string .

cal:NO2    a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .
cal:SO2    a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .
cal:CO     a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .
cal:PM10   a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .
cal:PM2_5  a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .
cal:O3     a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .
cal:BTX    a owl:DatatypeProperty ; rdfs:domain cal:Estacion ; rdfs:range xsd:string .


# Propiedades de Medición
cal:estaEnEstacion a owl:ObjectProperty ;
    rdfs:domain cal:Medicion ;
    rdfs:range cal:Estacion ;
    rdfs:label "realizada en estación" .

cal:PUNTO_MUESTREO a owl:DatatypeProperty ;
    rdfs:domain cal:Medicion ;
    rdfs:range xsd:string .

cal:ANO  a owl:DatatypeProperty ; rdfs:domain cal:Medicion ; rdfs:range xsd:gYear .
cal:MES  a owl:DatatypeProperty ; rdfs:domain cal:Medicion ; rdfs:range xsd:gMonth .
cal:DIA  a owl:DatatypeProperty ; rdfs:domain cal:Medicion ; rdfs:range xsd:gDay .

cal:MAGNITUD a owl:DatatypeProperty ;
    rdfs:domain cal:Medicion ;
    rdfs:range xsd:string ;
    rdfs:label "Magnitud medida" .


@prefix : <http://example.org/calidadAire/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

### CLASES ##############################

:Provincia a rdfs:Class ;
    rdfs:label "Provincia" .

:Municipio a rdfs:Class ;
    rdfs:label "Municipio" .

:Estacion a rdfs:Class ;
    rdfs:label "Estación de medición" .

:Magnitud a rdfs:Class ;
    rdfs:label "Magnitud contaminante" .

:Medicion a rdfs:Class ;
    rdfs:label "Medición diaria de calidad del aire" .

### PROPIEDADES DE OBJETO ################

:tieneMunicipio a rdf:Property ;
    rdfs:domain :Provincia ;
    rdfs:range :Municipio ;
    rdfs:label "tiene municipio" .

:tieneEstacion a rdf:Property ;
    rdfs:domain :Municipio ;
    rdfs:range :Estacion ;
    rdfs:label "tiene estación" .

:mideMagnitud a rdf:Property ;
    rdfs:domain :Estacion ;
    rdfs:range :Magnitud ;
    rdfs:label "mide magnitud" .

:tieneMedicion a rdf:Property ;
    rdfs:domain :Estacion ;
    rdfs:range :Medicion ;
    rdfs:label "tiene medición" .

### PROPIEDADES DE DATOS #################

:codigoProvincia a rdf:Property ;
    rdfs:domain :Provincia ;
    rdfs:range xsd:string .

:codigoMunicipio a rdf:Property ;
    rdfs:domain :Municipio ;
    rdfs:range xsd:string .

:codigoEstacion a rdf:Property ;
    rdfs:domain :Estacion ;
    rdfs:range xsd:string .

:codigoMagnitud a rdf:Property ;
    rdfs:domain :Magnitud ;
    rdfs:range xsd:string .

:puntoMuestreo a rdf:Property ;
    rdfs:domain :Medicion ;
    rdfs:range xsd:string .

:anio a rdf:Property ;
    rdfs:domain :Medicion ;
    rdfs:range xsd:gYear .

:mes a rdf:Property ;
    rdfs:domain :Medicion ;
    rdfs:range xsd:gMonth .

:dia a rdf:Property ;
    rdfs:domain :Medicion ;
    rdfs:range xsd:gDay .

:valorHora a rdf:Property ;
    rdfs:domain :Medicion ;
    rdfs:range xsd:float ;
    rdfs:label "valor horario" .

:estadoHora a rdf:Property ;
    rdfs:domain :Medicion ;
    rdfs:range xsd:string ;
    rdfs:label "estado de validez del dato" .